<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterEvent">

    <include
        android:id="@+id/include6"
        layout="@layout/logo_layout"
        android:layout_width="412dp"
        android:layout_height="106dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/regeventrecyclerView"
        android:layout_width="402dp"
        android:layout_height="403dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <TextView
        android:id="@+id/textViewfirstpos"
        android:layout_width="241dp"
        android:layout_height="26dp"
        android:layout_marginTop="13dp"
        android:hint="Selected Event"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regeventrecyclerView" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Register Event"
        android:onClick="registerEventfunc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewfirstpos" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="359dp"
        android:layout_height="23dp"
        android:layout_marginTop="7dp"
        android:text="Socials will be free after registering for one event"
        android:textColor="#EF6C00"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="219dp"
        android:layout_height="31dp"
        android:layout_marginTop="9dp"
        android:gravity="center"
        android:text="Events List"
        android:textColor="#EF6C00"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include6" />
</androidx.constraintlayout.widget.ConstraintLayout>